{
  "name": "cypress-cucumber-automation",
  "version": "2.2.0",
  "description": "This project is meant to be a kind of template to start working with Cucumber and Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:runner": "cypress open --e2e --browser chrome",
    "cypress:runner:headed": "cypress run --headed --browser chrome",
    "cypress:execution": "cypress run --spec cypress/e2e/features/*",
    "cypress:execution-tags": "cypress run --env tags=@mobile",
    "cypress:execution-allure": "cypress run --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "cypress:run:very website headed": "npx cypress run --spec cypress/e2e/features/very.feature --headed",
    "cypress:run:saucedo website headed": "npx cypress run --spec cypress/e2e/features/login.feature --headed",
    "cypress:run:BHMUnrecognizedCard headed": "npx cypress run --spec cypress/e2e/features/BHMUnrecognizedCard.feature --headed",
    "cypress:run:BHMSavingAddress headed": "npx cypress run --spec cypress/e2e/features/BHMSavingAddress.feature --headed",
    "cypress:run:computerDataBase headed": "npx cypress run --spec cypress/e2e/features/computerDataBase.feature --headed"
  },
  "keywords": [
    "cypress"
  ],
  "author": "cucumber",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@shelex/cypress-allure-plugin": "^2.5.0",
    "@types/dotenv": "^8.2.0",
    "cypress": "^13.5.1",
    "cypress-iframe": "^1.0.1",
    "cypress-wait-until": "^2.0.1",
    "cypress-xpath": "^2.0.1",
    "cypress-xray-plugin": "^5.2.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "multiple-cucumber-html-reporter": "^3.5.0"
  },
  "dependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-on-fix": "^1.0.2",
    "electron": "^27.0.1",
    "faker": "^5.5.3",
    "require.js": "^1.0.0",
    "sprintf-js": "^1.1.3"
  }
}
